{
  "name": "check-licenses",
  "version": "1.0.0",
  "description": "A Github action that detects new dependencies in PRs and adds reminder comments to review licensing details",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "build": "ncc build src/index.js -o dist --source-map --license licenses.txt",
    "local": "node src/index",
    "rebuild": "run-s clean build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oncai/check-licenses.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Dependency",
    "License"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oncai/check-licenses/issues"
  },
  "homepage": "https://github.com/oncai/check-licenses#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@octokit/action": "^4.0.10",
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "mustache": "^4.2.0",
    "node-fetch": "^3.3.0",
    "octokit": "^2.0.10",
    "parse-diff": "^0.10.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.0.0",
    "jest": "^27.2.5",
    "npm-run-all": "^4.1.5"
  }
}
